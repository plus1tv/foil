{
	"name": "@plus1tv/foil-cli",
	"version": "1.0.0-alpha.1",
	"description": "âœ¨ A sparkly shiny portfolio indexing system. Convert blog posts, art, music, etc. into database entries that you can traverse and search through.",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/plus1tv/foil.git"
	},
	"author": "Alain Galvan <hi@alain.xyz>",
	"license": "MIT",
	"typings": "dist/src/cli.d.ts",
	"types": "dist/foil-cli.d.ts",
	"source": "src/foil-cli.js",
	"main": "dist/foil-cli.js",
	"module": "dist/foil-cli.module.js",
	"unpkg": "dist/foil-cli.umd.js",
	"exports": "./dist/foil-cli.modern.js",
	"bin": {
		"foil-cli": "dist/foil-cli.js"
	},
	"engines": {
		"node": ">=8"
	},
	"ava": {
		"verbose": true,
		"extensions": [
			"ts"
		],
		"require": [
			"ts-node/register"
		],
		"files": [
			"tests/**/*.test.ts"
		]
	},
	"scripts": {
		"start": "npm run build",
		"build": "cross-env NODE_ENV=production microbundle ./src/cli.ts --compress  --target node --generateTypes",
		"develop": "tsc -w",
		"test": "c8 ava",
		"lint": "eslint ./src/**/*.ts",
		"prettier": "prettier --config .prettierrc --write \"src/**/*.{ts,tsx,js,json}\"",
		"coverage:local": "nyc npm test && nyc report",
		"coverage": "nyc npm test && nyc report --reporter=lcov && codecov"
	},
	"devDependencies": {
		"@types/chalk": "^2.2.0",
		"@types/node": "^16.11.6",
		"@types/react": "^17.0.33",
		"@types/react-redux": "^7.1.20",
		"@types/react-router": "^5.1.17",
		"@types/react-router-dom": "^5.3.2",
		"@types/rss": "^0.0.29",
		"@types/webpack": "^5.28.0",
		"@typescript-eslint/eslint-plugin": "^5.2.0",
		"ava": "^3.15.0",
		"c8": "^7.10.0",
		"clean-webpack-plugin": "^4.0.0",
		"codecov": "^3.8.x",
		"coveralls": "^3.1.x",
		"eslint": "^8.1.x",
		"lint-staged": "^11.2.x",
		"microbundle": "^0.14.1",
		"nyc": "^15.1.x",
		"prettier": "^2.4.x",
		"ts-node": "^10.4.0",
		"typescript": "4.4.4"
	},
	"dependencies": {
		"chalk": "^4.1.2",
		"cross-env": "^7.0.3",
		"dependency-tree": "^8.1.2",
		"feed": "^4.2.2",
		"file-loader": "^6.2.0",
		"find": "^0.3.0",
		"glob-to-regexp": "^0.4.1",
		"id3js": "^2.1.1",
		"markademic": "^0.15.2",
		"mongodb": "^4.1.3",
		"path-browserify": "^1.0.1",
		"rss": "^1.2.2",
		"ts-loader": "^9.2.6",
		"webpack": "^5.60.0"
	},
	"bugs": {
		"url": "https://github.com/alaingalvan/foil/issues"
	},
	"homepage": "https://github.com/alaingalvan/foil#readme",
	"directories": {
		"doc": "docs"
	}
}
