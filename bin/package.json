{
    "name": "foil-cli",
    "version": "1.0.0-alpha.0",
    "description": "âœ¨ A sparkly shiny portfolio indexing system. Convert blog posts, art, music, etc. into database entries that you can traverse and search through.",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/plus1tv/foil.git"
    },
    "author": "Alain Galvan <hi@alain.xyz>",
    "license": "MIT",
    "main": "./bin/src/cli.js",
    "bin": {
        "foil-cli": "./bin/src/cli.js"
    },
    "engines": {
        "node": ">=8"
    },
    "ava": {
        "verbose": true,
        "extensions": [
            "ts"
        ],
        "require": [
            "ts-node/register"
        ],
        "files": [
            "tests/**/*.test.ts"
        ]
    },
    "scripts": {
        "start": "npm run build && npm link --force",
        "build": "tsc",
        "develop": "tsc -w",
        "test": "c8 ava",
        "lint": "eslint ./src/**/*.ts",
        "prettier": "prettier --config .prettierrc --write \"src/**/*.{ts,tsx,js,json}\"",
        "coverage:local": "nyc npm test && nyc report",
        "coverage": "nyc npm test && nyc report --reporter=lcov && codecov"
    },
    "devDependencies": {
        "@types/chalk": "^2.2.0",
        "@types/mongodb": "^4.0.6",
        "@types/node": "^16.11.0",
        "@types/react": "^17.0.30",
        "@types/react-redux": "^7.1.19",
        "@types/react-router": "^5.1.17",
        "@types/react-router-dom": "^5.3.1",
        "@types/rss": "^0.0.29",
        "@types/webpack": "^5.28.0",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "ava": "^3.15.0",
        "c8": "^7.10.0",
        "codecov": "^3.8.x",
        "coveralls": "^3.1.x",
        "eslint": "^8.0.x",
        "lint-staged": "^11.2.x",
        "nyc": "^15.1.x",
        "prettier": "^2.4.x",
        "ts-node": "^10.3.0",
        "typescript": "4.4.4"
    },
    "dependencies": {
        "chalk": "^4.1.2",
        "cross-env": "^7.0.3",
        "dependency-tree": "^8.1.2",
        "file-loader": "^6.2.0",
        "find": "^0.3.0",
        "glob-to-regexp": "^0.4.1",
        "id3js": "^2.1.1",
        "markademic": "^0.14.3",
        "mongodb": "^4.1.3",
        "path-browserify": "^1.0.1",
        "rss": "^1.2.2",
        "ts-loader": "^9.2.6",
        "webpack": "^5.58.2"
    },
    "bugs": {
        "url": "https://github.com/alaingalvan/foil/issues"
    },
    "homepage": "https://github.com/alaingalvan/foil#readme",
    "directories": {
        "doc": "docs"
    }
}
